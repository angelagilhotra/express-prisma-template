generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model testimonials {
  source       String? @db.VarChar
  text         String? @db.VarChar
  author_image String? @db.VarChar
  url          String? @db.VarChar
  id           Int     @id @default(autoincrement())
}
